<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--绘制轨迹是否可见-->
    <attr name="hasTrack" format="boolean" />
    <!--点击是否有声音-->
    <attr name="hasSound" format="boolean" />
    <!--点击是否有震动-->
    <attr name="hasShake" format="boolean" />

    <!--九宫格中用到的-->
    <declare-styleable name="lockPattern">
        <attr name="pointCount" format="integer" />
        <attr name="hasTrack" />
        <attr name="hasSound" />
        <attr name="hasShake" />
    </declare-styleable>

    <!--数字解锁界面所用-->
    <declare-styleable name="LockNumber">
        <attr name="hasTrack" />
        <attr name="hasSound" />
        <attr name="hasShake" />
    </declare-styleable>
</resources>